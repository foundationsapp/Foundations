<?xml version="1.0" encoding="utf-8"?>

<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
xmlns:app="http://schemas.android.com/apk/res-auto"
xmlns:tools="http://schemas.android.com/tools"
android:layout_width="match_parent"
android:layout_height="match_parent"
tools:context=".MainActivity">

<LinearLayout
    android:background="@color/background"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical">

    <ImageView
        android:layout_marginTop="@dimen/_50"
        android:layout_width="@dimen/_100"
        android:layout_height="@dimen/_100"
        android:layout_gravity="center"
        />

    <TextView
        android:layout_marginTop="@dimen/_20"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/foundations"
        android:fontFamily="sans-serif-black"
        android:textStyle="bold"
        android:textSize="@dimen/_50"
        android:textColor="@color/colorPrimaryDark"
        android:layout_gravity="center"/>
    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/recent_inspections"
        android:textStyle="bold"
        android:textSize="@dimen/_20"
        android:textColor="@color/sloganColor"
        android:layout_gravity="center"/>

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/_123_fairy_tale_lane_seattle_wa_98111"
        android:textStyle="bold"
        android:textSize="@dimen/_20"
        android:textColor="@color/sloganColor"
        android:layout_gravity="center"/>

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/_321_fake_st_federal_wa_98888"
        android:textStyle="bold"
        android:textSize="@dimen/_20"
        android:textColor="@color/sloganColor"
        android:layout_gravity="center"/>

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/_888_rocket_dr_cloud_tx_77665"
        android:textStyle="bold"
        android:textSize="@dimen/_20"
        android:textColor="@color/sloganColor"
        android:layout_gravity="center"/>

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/_1_twothree_ave_portland_or_97035"
        android:textStyle="bold"
        android:textSize="@dimen/_20"
        android:textColor="@color/sloganColor"
        android:layout_gravity="center"/>



    <Button
        android:id="@+id/button"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="0dp"

        android:text="@string/open_activity_2"
        tools:ignore="OnClick" />

</LinearLayout>

</androidx.constraintlayout.widget.ConstraintLayout>

